<form method="POST">
   <div class="form-group">
      <label for="uname">Username</label>
      <input class="form-control" aria-describedby="uError" type="text" name="uname" id="uname" placeholder="Enter Username">
      <!-- --> <small id="uError"  class="form-text text-muted ">Something Wrong</small>
   </div>
   <div class="form-group">
      <label for="pass">Password</label>
      <input class="form-control" aria-describedby="pError" type="password" name="pass" id="pass" placeholder="Enter Password">
      <!-- --> <small id="uError" class="form-text text-muted">Something Wrong</small>
   </div>
   <div class="form-check">
      <input type="checkbox" class="form-check-input" id="remember_me">
      <label class="form-check-label" for="remember_me">Remember Me</label>
    </div>
    <button type="submit" class="btn btn-primary">Login</button>
 </form>

