<div class="row">
    <div class="container col-md-9">
        <form method="POST" class="container shadow-lg p-3 mb-5 bg-white rounded">
            <div class="form-group">
                <label>Name</label>
                <input type="text" class="form-control form-control-lg" id="name" name="name" aria-describedby="e" placeholder="" value="<%= userInfo.name%>" required>
            </div>
            <div class="form-group">
                <label>User Name</label>
                <input type="text" class="form-control form-control-lg" id="username" name="username" aria-describedby="e" placeholder="" value="<%= userInfo.username%>" readonly>
            </div>
            <div class="form-group">
                <label>Rank</label>
                <input type="text" class="form-control form-control-lg" id="rank" name="rank" aria-describedby="e" placeholder="" value="<%= userInfo.rank%>" readonly>
            </div>
            <div class="form-group">
                <label>Contact No.</label>
                <input type="text" class="form-control form-control-lg" id="contact" name="contact" aria-describedby="e" placeholder="" value="<%= userInfo.contact%>" required>
            </div>
            <div class="form-group">
                <label>Password</label>
                <input type="text" class="form-control form-control-lg" id="password" name="password" aria-describedby="e" placeholder="" value="<%= userInfo.password%>" required>
            </div>
            <table align="center">
                <tr>
                    <td>
                        <button type="submit" class="btn btn btn-outline-success">Save</button>
                        <a href="/manager/home" class="btn btn-outline-primary">Cancel</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>
